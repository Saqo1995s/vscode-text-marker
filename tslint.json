{
  "rulesDirectory": ["./node_modules/fp-ts/rules"],
  "rules": {
    "no-string-throw": {},
    "no-unused-expression": {},
    "no-duplicate-variable": {},
    "curly": {"options": ["ignore-same-line"]},
    "class-name": {},
    "semicolon": {"options": ["always"]},
    "quotemark": {"options": ["single", "avoid-escape"]},
    "triple-equals": {},
    "obsolete": {"severity": "warning"}
  },
  "defaultSeverity": "error"
}
